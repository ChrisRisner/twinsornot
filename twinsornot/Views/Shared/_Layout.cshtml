    <script type="text/javascript">

    $(function () {
        $('#fileUpload').fileupload({
            dataType: 'json',
            singleFileUploads: false,
            url: '@Url.Action("upload")',
            previewMaxWidth: 200,
            previewMaxHeight: 200,
            previewCrop: true
        }).on('fileuploadadd', function (e, data) {

            try {
                result.innerText = "Analyzing";
                if (data.files && data.files[0] && data.files[1]) {
                    var reader = new FileReader();
                    reader.onload = function (e) {
                        $('#photo1').attr('src', e.target.result);
                    }
                    reader.readAsDataURL(data.files[0]);


                    var reader2 = new FileReader();
                    reader2.onload = function (e) {
                        $('#photo2').attr('src', e.target.result);
                    }
                    reader2.readAsDataURL(data.files[1]);
                    data.submit();
                }
            } catch (e) {
                window.alert(e);
            }
        }).on('fileuploadprocessalways', function (e, data) {
            var index = data.index,
                file = data.files[index],
                node = $(data.context.children()[index]);
            if (file.preview) {
                node
                    .prepend('<br>')
                    .prepend(file.preview);
            }
           
        }).on('fileuploaddone', function (e, data) {
            
            if (data.result.error != "") {
                result.innerText = data.result.error;
            }
            else {
                if (data.result.result == 100) {
                    result.innerText = "Twin score: 100% OMG, clones!!!";
                } else if (data.result.result > 90) {
                    result.innerText = "Twin score: " + data.result.result + "%, almost identical!";
                } else if (data.result.result > 50) {
                    result.innerText = "Twin score: " + data.result.result + "%, you two are definitely related.";
                } else if (data.result.result > 20) {
                    result.innerText = "Twin score: " + data.result.result + "%, we can see some resemblance.";
                }
                else {
                    result.innerText = "Twin score: " + data.result.result + "%, probably not even from the same planet.";
                }

            }

            });
    });
    </script>
